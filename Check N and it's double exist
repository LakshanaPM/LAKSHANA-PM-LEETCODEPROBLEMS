bool checkIfExist(int* arr, int arrSize) {
    int i;
    for(i=0;i<arrSize-1;i++){
        for(int j=i+1;j<arrSize;j++){
            if(arr[i]==arr[j]*2){
                return true;
            }
            else if(arr[i]*2==arr[j]){
                return true;
            }
        }
    }
   
    return false;
}
